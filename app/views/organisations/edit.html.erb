<div class="card">
  <div class="card-header">
    <h1>Edit Organisation</h1>
  </div>
  <div class="card-body">
    <%= render "form", organisation: @organisation %>
    <br>
    <div class="btn-toolbar">
      <%= link_to 'About', @organisation, class:"btn btn-secondary btn-group mr-2" %>
      <%= button_to 'Unsubscribe', @organisation, method: :delete, class: "btn btn-danger btn-group mr-2" %>
      <%= link_to 'Back', :back, class:"btn btn-dark btn-group mr-2" %>
    </div>
  </div>
</div>
